<Project>
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <!-- <PublishAot>true</PublishAot> -->
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <InvariantGlobalization>true</InvariantGlobalization>

    <DefineConstants Condition="'$(GENERATING_OPENAPI)' == 'true'">$(DefineConstants);GENERATING_OPENAPI</DefineConstants>

    <NoWarn>
      TUnit0046 <!-- Data source method should return Func<T> for reference type parameters -->
    </NoWarn> 
    <WarningsNotAsErrors>
      CS9270 <!-- 'InterceptsLocationAttribute(string, int, int)' is not supported. Move to 'InterceptableLocation'-based generation of these attributes instead. (https://github.com/dotnet/roslyn/issues/72133) -->
      CS8669 <!-- The annotation for nullable reference types should only be used in code within a '#nullable' annotations context. Auto-generated code requires an explicit '#nullable' directive in source. -->
    </WarningsNotAsErrors>
  </PropertyGroup>
</Project>